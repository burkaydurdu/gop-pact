{
  "consumer": {
    "name": "GopClient"
  },
  "provider": {
    "name": "GopServer"
  },
  "interactions": [
    {
      "description": "A request for campaign",
      "providerState": "i get new product price with specified discount rate",
      "request": {
        "method": "GET",
        "path": "/products/1/discount",
        "query": "rate=30",
        "matchingRules": {
          "$.query.rate[0]": {
            "match": "type"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "id": 42,
          "name": "",
          "price": 42
        },
        "matchingRules": {
          "$.body.id": {
            "match": "type"
          },
          "$.body.name": {
            "match": "type"
          },
          "$.body.price": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}